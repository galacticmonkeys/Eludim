{
  "manifest_version": 2,

  "name": "Eludim",
  "description": "Dim lightbox modals",
  "version": "0.1",

  "background": {
    "scripts" : [
        "js/jquery-1.11.3.min.js",
        "js/background.js"
    ]
  },

  "browser_action": {
   "default_icon": "img/iconGray.png",
   "default_popup": "popup.html"
  },

  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png" 
  },

  "content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["js/jquery-1.11.3.min.js" , "js/content.js"],
    "run_at": "document_end"
    }
  ], 

  "permissions": [
    "tabs",
    "storage",
    "<all_urls>",
    "management"
  ]
}
