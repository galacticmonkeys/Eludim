{
  "manifest_version": 2,

  "name": "Eludim",
  "description": "Bypass lightbox modals.",
  "version": "0.1",

  "browser_action": {
   "default_icon": "img/icon.png",
   "default_popup": "popup.html"
  },

  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png" 
  },

  "content_scripts": [
    {
    "matches": [
        "http://*/*",
        "https://*/*"
        ],
    "js": ["js/jquery-1.11.3.min.js" , "js/content.js"],
    "run_at": "document_end"
    }
  ], 

  "permissions": [
    "tabs","<all_urls>"
  ]
}
